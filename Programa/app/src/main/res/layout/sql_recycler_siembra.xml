<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="1dp"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="89dp"
        android:orientation="horizontal"
        android:padding="12dp">

        <RelativeLayout
            android:layout_width="789dp"
            android:layout_height="56dp">

            <LinearLayout
                android:layout_width="774dp"
                android:layout_height="45dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txtViewBloqueRecycler"
                    android:layout_width="82dp"
                    android:layout_height="wrap_content"
                    android:text="Bloque"
                    android:textSize="23sp" />

                <TextView
                    android:id="@+id/txtViewCamaRecycler"
                    android:layout_width="78dp"
                    android:layout_height="wrap_content"
                    android:text="Cama"
                    android:textSize="23sp" />

                <TextView
                    android:id="@+id/txtViewVariedadRecycler"
                    android:layout_width="195dp"
                    android:layout_height="wrap_content"
                    android:text="Variedad"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/txtViewProce"
                    android:layout_width="110dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Proce"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/txtViewTipoSIembraRecycler"
                    android:layout_width="102dp"
                    android:layout_height="wrap_content"
                    android:text="tipo"
                    android:textSize="21sp" />

                <TextView
                    android:id="@+id/txtViewMetrosRecycler"
                    android:layout_width="85dp"
                    android:layout_height="wrap_content"
                    android:text="Metros"
                    android:textSize="23sp" />

                <TextView
                    android:id="@+id/txtViewCalibreRecycler"
                    android:layout_width="110dp"
                    android:layout_height="wrap_content"
                    android:text="Calibre"
                    android:textSize="23sp" />

                <ImageView
                    android:id="@+id/imagenEditar"
                    android:layout_width="31dp"
                    android:layout_height="45dp"
                    app:srcCompat="@drawable/botoneditar" />
            </LinearLayout>
        </RelativeLayout>


    </LinearLayout>
</androidx.cardview.widget.CardView>